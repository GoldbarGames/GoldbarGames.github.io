{
  "name": "goldbar-games",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">= 0.10.0"
  },
  "devDependencies": {
    "masonry-layout": "^4.2.0",
    "npm-run-all": "^1.7.0",
    "onchange": "^2.2.0",
    "tablesaw": "^3.0.6",
    "uglify-js": "^2.6.1"
  },
  "scripts": {
    "uglify": "uglifyjs assets/js/vendor/jquery/jquery-3.2.1.min.js assets/js/plugins/jquery.fitvids.js assets/js/plugins/jquery.greedy-navigation.js assets/js/plugins/jquery.magnific-popup.js assets/js/plugins/jquery.smooth-scroll.min.js node_modules/masonry-layout/dist/masonry.pkgd.js node_modules/tablesaw/dist/stackonly/tablesaw.stackonly.jquery.js node_modules/tablesaw/dist/tablesaw-init.js assets/js/_main.js -c -m -o assets/js/main.min.js",
    "watch:js": "onchange \"assets/js/**/*.js\" -e \"assets/js/main.min.js\" -- npm run build:js",
    "build:js": "npm run uglify"
  }
}